@model CoreWeb.Models.EmployeeModel

@{
    ViewData["Title"] = "Details From DB";
}

<h2>Employee Details From DB</h2>

<table style="font-family: Arial; border:solid">
    <tr>
        <td><b>EmployeeId: </b></td>
        <td>@Model.EmployeeId</td>
    </tr>
    <tr>
        <td><b>Name: </b></td>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <td><b>Gender: </b></td>
        <td>@Model.Gender</td>
    </tr>
    <tr>
        <td><b>City: </b></td>
        <td>@Model.City</td>
    </tr>

</table>
